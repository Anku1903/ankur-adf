{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "adls_adf_dest",
						"type": "DatasetReference"
					},
					"name": "DFsource"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "dataflow_sink",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "filter1"
				},
				{
					"name": "aggregate1"
				},
				{
					"name": "sort1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          user_id as string,",
				"          session_id as string,",
				"          event_time as string,",
				"          event_type as string,",
				"          page_or_screen as string,",
				"          referrer as string,",
				"          location as string,",
				"          os as string,",
				"          device_model as string,",
				"          duration_sec as double",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     wildcardPaths:['*.csv']) ~> DFsource",
				"DFsource filter(duration_sec > 5) ~> filter1",
				"filter1 aggregate(groupBy(os,",
				"          event_type),",
				"     session_id = count(session_id)) ~> aggregate1",
				"aggregate1 sort(asc(os, true),",
				"     asc(event_type, true)) ~> sort1",
				"sort1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sink1"
			]
		}
	}
}