{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ankur-adf"
		},
		"PostgreSql aiven_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'PostgreSql aiven'"
		},
		"Shopify1_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'Shopify1'"
		},
		"adf_adls_connect_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'adf_adls_connect'"
		},
		"PostgreSql aiven_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "pg-ankur-ankurzalavadiya11-65ec.d.aivencloud.com"
		},
		"PostgreSql aiven_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "defaultdb"
		},
		"PostgreSql aiven_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "avnadmin"
		},
		"RestAPIService_Products_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dummyjson.com"
		},
		"Shopify1_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "grow-men.myshopify.com"
		},
		"adf_adls_connect_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://ankurdataenggstorage.dfs.core.windows.net/"
		},
		"pokeapi_ls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://pokeapi.co/api/v2/pokemon?limit=20&offset=0"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ankur-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"PostgreSql aiven_password": {
						"value": "[parameters('PostgreSql aiven_password')]"
					},
					"Shopify1_accessToken": {
						"value": "[parameters('Shopify1_accessToken')]"
					},
					"adf_adls_connect_accountKey": {
						"value": "[parameters('adf_adls_connect_accountKey')]"
					},
					"PostgreSql aiven_properties_typeProperties_server": {
						"value": "[parameters('PostgreSql aiven_properties_typeProperties_server')]"
					},
					"PostgreSql aiven_properties_typeProperties_database": {
						"value": "[parameters('PostgreSql aiven_properties_typeProperties_database')]"
					},
					"PostgreSql aiven_properties_typeProperties_username": {
						"value": "[parameters('PostgreSql aiven_properties_typeProperties_username')]"
					},
					"RestAPIService_Products_properties_typeProperties_url": {
						"value": "[parameters('RestAPIService_Products_properties_typeProperties_url')]"
					},
					"Shopify1_properties_typeProperties_host": {
						"value": "[parameters('Shopify1_properties_typeProperties_host')]"
					},
					"adf_adls_connect_properties_typeProperties_url": {
						"value": "[parameters('adf_adls_connect_properties_typeProperties_url')]"
					},
					"pokeapi_ls_properties_typeProperties_url": {
						"value": "[parameters('pokeapi_ls_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}